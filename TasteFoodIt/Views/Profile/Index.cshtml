@model TasteFoodIt.Entities.Admin
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
    int count = 1;
}

<div class="container-fluid" id="container-wrapper">
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Profil Bilgileri</h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/Default/Index/">Ana Sayfa</a></li>
            <li class="breadcrumb-item"><a href="/Product/ProductList/">Profilim</a></li>
            @*<li class="breadcrumb-item active" aria-current="page">Şifre Değiştir</li>*@
        </ol>
    </div>

    <div class="row">
        <div class="col-lg-12 mb-4">
            <!-- Simple Tables -->
            <div class="card">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">

                </div>
                <div class="table-responsive">
                    <table class="table align-items-center table-flush">
                        <thead class="thead-light">
                            <tr>
                                <th>Id</th>
                                <th>İsim Soyisim</th>
                                <th>Kullanıcı Adı</th>
                                <th>Şifre</th>
                                <th>Resim</th>
                            </tr>
                        </thead>
                        <tbody>
                            
                              
                                <tr>

                                    <td>@count</td>
                                    <td>@ViewBag.c</td>
                                    <td>@ViewBag.v</td>
                                    <td>@ViewBag.p</td>
                                    <td><img src="@ViewBag.x" alt="admin" width="75px" height="75px" /></td>
                                </tr>
                            
                        </tbody>
                    </table>
                    <br />
                    
                </div>
                <div class="card-footer"></div>
            </div>
        </div>
    </div>

   

</div>

