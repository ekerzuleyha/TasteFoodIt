@model TasteFoodIt.Entities.ContactInformation
@{
    ViewBag.Title = "CreateContactInformation";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="container-fluid" id="container-wrapper">
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Adres Bilgileri Ekleme Sayfası</h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/Default/Index/">Ana Sayfa</a></li>
            <li class="breadcrumb-item">Adres Bilgileri</li>
            <li class="breadcrumb-item active" aria-current="page">Adres Bilgileri İşlemleri</li>
        </ol>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <!-- Form Basic -->
            <div class="card mb-4">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                    <h6 class="m-0 font-weight-bold text-primary">Ekleme Paneli</h6>
                </div>
                <div class="card-body">
                    <form method="post">
                        <div class="form-group">
                            <label>Adres</label>
                            <input type="text" class="form-control" name="Address">
                            <br />

                            <label>Telefon</label>
                            <input type="text" class="form-control" name="Phone">
                            <br />

                            <label>Email</label>
                            <input type="text" class="form-control" name="Email">
                            <br />

                            <label>Website</label>
                            <input type="text" class="form-control" name="WebsiteUrl">
                            <br />

                        </div>
                        
                            <button type="submit" class="btn btn-primary" id="deneme">Adres Bilgisi Ekle</button>
                        

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

